{
  "name": "@fermuch/monoutils",
  "version": "0.1.4",
  "description": "Utilities for Monoflow",
  "main": "build/index.js",
  "types": "build/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "webpack serve --config webpack.config.demo.js",
    "build": "webpack && tsc",
    "build:demo": "webpack --config webpack.config.demo.js",
    "test": "jest --silent",
    "coverage": "npm run test -- --coverage",
    "prepare": "npm run build",
    "trypublish": "npm publish || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fermuch/monoutils.git"
  },
  "author": "Fernando Mumbach <fermuch@ayvu.net> (https://github.com/fermuch)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fermuch/monoutils/issues"
  },
  "homepage": "https://github.com/fermuch/monoutils",
  "keywords": [
    "library",
    "monoflow"
  ],
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.10",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-transform-typescript": "^7.26.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.26.9",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^10.0.0",
    "babel-preset-minify": "^0.5.2",
    "brotli-webpack-plugin": "^1.1.0",
    "compression-webpack-plugin": "^11.1.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.23.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "mobx-keystone": "^1.11.3",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "prettier": "^2.8.8",
    "prettier-webpack-plugin": "^1.2.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.2",
    "typescript": "^4.9.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "5.2.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/scripts/testMock.js",
      "\\.(css|less)$": "<rootDir>/scripts/testMock.js"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/private/jest/setup.ts"
    ]
  },
  "dependencies": {
    "@fermuch/telematree": "^14.0.0",
    "typed-emitter": "^2.1.0"
  }
}
